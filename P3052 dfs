#include <bits/stdc++.h>
#include <cstdlib>
using namespace std;
#define int long long
#define piii pair<int,int>
//sort(puch,puch+n,greater<int>());
struct place{
	int u,v;
};
vector<place>s;
bool cmp(place x,place y){
	if(x.v==y.v){
		return x.u<y.u;
	}
	else return x.v<y.v;
}
int n,w,a1,a2,b1,b2;
int ans=1000000;
int arr[1001];
int sum[1001];
int puch[1001];
int dx[4]={1,-1,0,0}, dy[4]={0,0,1,-1};
void dfs(int u,int v){
	if(v>=ans){
		return;
	}
	if(u==n){
		ans=v;
		return;
	}
	for(int i=0;i<v;i++){
		if(sum[i]+puch[u]<=w){
			sum[i]+=puch[u];
			dfs(u+1,v);
			sum[i]-=puch[u];
		}
	}
	sum[v]=puch[u];
	dfs(u+1,v+1);
	sum[v]=0;
	
}
signed main(){
	cin>>n>>w;
	for(int i=0;i<n;i++){
		cin>>puch[i];
		
	}
	sort(puch,puch+n,greater<int>());
	dfs(0,0);
	cout<<ans<<endl;
}      
